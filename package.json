  "type": "module",
  "scripts": {
    "knip": "npx knip",
    "dev": "vite",
    "makeconfig": "echo '' > public/config.js",
    "cypress:open": "yarn run cypress open",
    "cypress:e2e": "yarn run cypress open",
    "buildci": "yarn makeconfig && yarn build && yarn targz",
    "build": "yarn lingui && vite build",
    "preview": "yarn build && vite preview",
    "targz": "tar -zcvf dist.tar.gz -C dist .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "fmt": "yarn prettier:write",
    "prettier:check": "./node_modules/.bin/prettier --check .",
    "prettier:write": "./node_modules/.bin/prettier --cache --write .",
    "add-locale": "lingui add-locale",
    "prune-locale": "lingui extract --clean",
    "lingui": "lingui extract",
    "DUMMY": "",
    "lint:check": "tsc --noEmit && eslint 'src/**/*.ts{,x}' --cache --max-warnings=0",
    "lint:fix": "eslint 'src/**/*.ts{,x}' --cache --max-warnings=0 --fix",
    "generate": "yarn generate:all",
    "generate:all": "yarn generate:canoe && yarn generate:wagmi",
    "generate:canoe": "yarn openapi --input https://oku-canoe.fly.dev/openapi.json -o src/canoe --name CanoeClient",
    "generate:wagmi": "yarn wagmi generate",
    "prepare": "husky"
  },
